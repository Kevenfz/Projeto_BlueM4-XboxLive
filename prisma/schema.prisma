generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Games {
  id String @id @default(uuid())
  title String @unique
  imgUrl String @unique
  description String @unique
  year String
  score Int
  traillerYtUrl String @unique
  GplayYtUrl String @unique
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Genero {
  id String @id @default(uuid())
  genero String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
